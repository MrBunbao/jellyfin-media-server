{
  "name": "DD",
  "includeCustomFormatWhenRenaming": false,
  "specifications": [
    {
      "name": "Dolby Digital",
      "implementation": "ReleaseTitleSpecification",
      "negate": false,
      "required": true,
      "fields": {
        "value": "\\b(DD[^P+]|(?<!E-)AC-?3|Dolby.?Digital)\\b"
      }
    },
    {
      "name": "Not Atmos",
      "implementation": "ReleaseTitleSpecification",
      "negate": true,
      "required": true,
      "fields": {
        "value": "\\b(ATMOS)\\b"
      }
    }
  ]
}
